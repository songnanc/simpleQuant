{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "clean": "rm -rf dist && mkdir dist",
    "serve": "http-server . -p 5556",
    "build": "npm run clean && tsc",
    "build_prod": "npm run build && browserify -s main app/main.js > dist/bundle.js && npm run minify",
    "webpack": "npm run clean && webpack && npm run minify",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "pree2e": "webdriver-manager update",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.3.0",
    "@angular/compiler": "~2.3.0",
    "@angular/core": "~2.3.0",
    "@angular/forms": "~2.3.0",
    "@angular/http": "~2.3.0",
    "@angular/platform-browser": "~2.3.0",
    "@angular/platform-browser-dynamic": "~2.3.0",
    "@angular/router": "~3.3.0",
    "angular-confirmation-popover": "^2.1.2",
    "angular-in-memory-web-api": "~0.1.15",
    "chart.js": "^2.4.0",
    "core-js": "^2.4.1",
    "ng2-bootstrap": "^1.2.2",
    "ng2-charts": "^1.5.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-rc.4",
    "systemjs": "0.19.40",
    "zone.js": "^0.7.0"
  },
  "devDependencies": {
    "@types/jasmine": "=2.5.41",
    "@types/node": "^6.0.46",
    "@types/selenium-webdriver": "^2.53.33",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.4",
    "browserify": "^13.1.1",
    "canonical-path": "0.0.2",
    "codemirror": "^5.25.2",
    "concurrently": "^3.1.0",
    "css-loader": "^0.23.1",
    "html-loader": "^0.4.3",
    "http-server": "^0.9.0",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "ng2-codemirror": "^1.1.1",
    "protractor": "4.0.9",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "tslint": "^3.15.1",
    "typescript": "=2.0.3",
    "uglify-js": "^2.7.5",
    "webdriver-manager": "10.2.5",
    "webpack": "^2.2.0"
  },
  "repository": {}
}
